if x == c :

a == gukbabchung(n)
b == Lotteria(n)
c == Zzazzarazzazza(n)

def Tteokbokki(n) :
    return n//3500
n=int(input("당신이 n년 뒤 받을 돈을 입력해 주십시오"))
print("그 돈이면 떡볶이를",Tteokbokki(n),"그릇을 먹겠다")

y = input('드실 음식을 적어주세요:')
z = int('해당 음식의 가격을 적어주세요:')

def y(n) :
    return n//z
n=int(input("당신이 n년 뒤 받을 돈을 입력해 주십시오"))
print("그 돈이면 떡볶이를",Tteokbokki(n),"그릇을 먹겠다")

